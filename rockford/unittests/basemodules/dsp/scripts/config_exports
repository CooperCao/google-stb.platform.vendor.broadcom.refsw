elif [ $chipnum == "7271" ]
then
        export NEXUS_USE_7271_SV=y
	export PLATFORM=97271
	export SAGE_SUPPORT=n
	export RAP_IQA_TEST_SUPPORT=y
	export NEXUS_ARM_AUDIO_SUPPORT=y
	export BDSP_AC4_SUPPORT=y
	export BDSP_MS12_SUPPORT=A

elif [ $chipnum == "7278" ]
then
    export MEDIA_ASF_SUPPORT=n
    export PLAYBACK_IP_SUPPORT=n
    export NEXUS_USE_7278_SV=y
    export PLATFORM=97278
    export BDSP_VP6_SUPPORT=n
    export VIDEO_ENCODER_SUPPORT=y
    export BDSP_MS12_SUPPORT=y
    export BMUXLIB_INPUT_DUMP_DESC=y
    export BMUXLIB_INPUT_DUMP_FRAME_DESC=y
    export NEXUS_SECURITY_SUPPORT=n
    export NEXUS_VBI_SUPPORT=n
    export NXCLIENT_SUPPORT=n

elif [ $chipnum == "7260" ]
then
	export BDSP_MS12_SUPPORT=D
	export BDSP_RAAGA_AUDIO_SUPPORT=n
	export BDSP_ARM_ONLY_SOLUTION=y
	export NEXUS_ARM_AUDIO_SUPPORT=y
	export SAGE_SUPPORT=n
	export VIDEO_ENCODER_SUPPORT=n
	export BDSP_ENCODER_SUPPORT=n

elif [ $chipnum == "7255" ]
then
      export BDSP_MS11PLUS_SUPPORT=y
      export BDSP_RAAGA_AUDIO_SUPPORT=n
      export NEXUS_ARM_AUDIO_SUPPORT=y
      export SAGE_SUPPORT=n

elif [ $chipnum == "7445" ]
then
	export MEDIA_ASF_SUPPORT=y
	export MEDIA_AVI_SUPPORT=y
	export MEDIA_FLV_SUPPORT=y
	export VIDEO_ENCODER_SUPPORT=y
	export NEXUS_MS12_SUPPORT=y
	export BDSP_VP6_SUPPORT=y
	export BDSP_AACSBR_SUPPORT=n
	export BDSP_MS10_SUPPORT=n
	export BDSP_MS11_SUPPORT=n
	export BDSP_MS12_SUPPORT=y
	export BDSP_AC4_SUPPORT=y
	export BMUXLIB_INPUT_DUMP_DESC=y
	export BMUXLIB_INPUT_DUMP_FRAME_DESC=y
	export RAP_IQA_TEST_SUPPORT=y
	export SAGE_SUPPORT=n
	export PLATFORM=97445

elif [ $chipnum == "7211" ]
then
    export BDSP_MS11PLUS_SUPPORT=n
    export BDSP_MS11_SUPPORT=n
    export BDSP_MS12_SUPPORT=n
    export BDSP_AACSBR_SUPPORT=y
    export BDSP_RAAGA_AUDIO_SUPPORT=n
    export NEXUS_ARM_AUDIO_SUPPORT=y
    export BDSP_SOFT_FMM_SUPPORT=y

else
	echo "ERROR: Exports are not defined for chipnum: $chipnum."
fi
