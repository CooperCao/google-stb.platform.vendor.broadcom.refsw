chipnum=$1
#unset the all export settings. This list should be modified as and when new exports are added
unset HLS_PROTOCOL_SUPPORT
unset LIVEMEDIA_SUPPORT
unset LIVE_STREAMING_SUPPORT
unset MEDIA_ASF_SUPPORT
unset MEDIA_AVI_SUPPORT
unset MEDIA_FLV_SUPPORT
unset NETACCEL_SUPPORT
unset NEXUS_POWER_MANAGEMENT
unset PLAYBACK_IP_SUPPORT
unset VIDEO_ENCODER_SUPPORT
unset BDSP_VP6_SUPPORT
unset BMUXLIB_INPUT_DUMP_DESC
unset BMUXLIB_INPUT_DUMP_FRAME_DESC
unset BDSP_MS10_SUPPORT
unset BDSP_MS11_SUPPORT
unset BDSP_MS12_SUPPORT
unset BDSP_AC4_SUPPORT
unset BDSP_ENCODER_SUPPORT
unset PLATFORM
unset RAP_IQA_TEST_SUPPORT
unset BDSP_AACSBR_SUPPORT
unset NEXUS_MS12_SUPPORT
unset NEXUS_USE_7364_SV
unset BDSP_ENCODER_ACCELERATOR_SUPPORT
unset SAGE_SUPPORT
unset NEXUS_ARM_AUDIO_SUPPORT
unset MRC_MONITOR
unset NXCLIENT_SUPPORT
unset BDSP_SOFT_FMM_SUPPORT
#####################################
if [ $chipnum == "7425" ]
then
	export MEDIA_ASF_SUPPORT=y
	export MEDIA_AVI_SUPPORT=y
	export MEDIA_FLV_SUPPORT=y
	export VIDEO_ENCODER_SUPPORT=y
	export BDSP_VP6_SUPPORT=y
	export BDSP_MS10_SUPPORT=y
	export BDSP_MS11_SUPPORT=y
	export BMUXLIB_INPUT_DUMP_DESC=y
	export BMUXLIB_INPUT_DUMP_FRAME_DESC=y
	export PLATFORM=97425
	export RAP_IQA_TEST_SUPPORT=y
	export SAGE_SUPPORT=n
	export MRC_MONITOR=y

elif [ $chipnum == "7358" ]
then
	export MEDIA_AVI_SUPPORT=y
	export MEDIA_ASF_SUPPORT=y
	export MEDIA_FLV_SUPPORT=y
	export BDSP_MS10_SUPPORT=n
	export BDSP_MS11_SUPPORT=n
	export BMUXLIB_INPUT_DUMP_DESC=y
	export BMUXLIB_INPUT_DUMP_FRAME_DESC=y
	export PLATFORM=97358
	export SAGE_SUPPORT=n
	export RAP_IQA_TEST_SUPPORT=y

elif [ $chipnum == "7346" ]
then
	export MEDIA_ASF_SUPPORT=y
	export MEDIA_AVI_SUPPORT=y
	export MEDIA_FLV_SUPPORT=y
	export VIDEO_ENCODER_SUPPORT=y
	export BDSP_ENCODER_SUPPORT=y
	export BDSP_MS10_SUPPORT=y
	export BDSP_MS11_SUPPORT=y
	export BMUXLIB_INPUT_DUMP_DESC=y
	export BMUXLIB_INPUT_DUMP_FRAME_DESC=y
	export PLATFORM=97346
	export SAGE_SUPPORT=n
	export RAP_IQA_TEST_SUPPORT=y

elif [ $chipnum == "7435" ]
then
	export MEDIA_ASF_SUPPORT=y
	export MEDIA_AVI_SUPPORT=y
	export MEDIA_FLV_SUPPORT=y
	export VIDEO_ENCODER_SUPPORT=y
	export BDSP_VP6_SUPPORT=y
	export BDSP_MS10_SUPPORT=y
	export BDSP_MS11_SUPPORT=y
	export BMUXLIB_INPUT_DUMP_DESC=y
	export BMUXLIB_INPUT_DUMP_FRAME_DESC=y
	export PLATFORM=97435
	export RAP_IQA_TEST_SUPPORT=y
	export SAGE_SUPPORT=n
	export MRC_MONITOR=y

elif [ $chipnum == "7364" ]
then
        export MEDIA_ASF_SUPPORT=y
        export MEDIA_AVI_SUPPORT=y
        export MEDIA_FLV_SUPPORT=y
	export PLAYBACK_IP_SUPPORT=n
	export VIDEO_ENCODER_SUPPORT=y
	export BDSP_ENCODER_SUPPORT=y
	export BDSP_ENCODER_ACCELERATOR_SUPPORT=y
        export NEXUS_POWER_MANAGEMENT=n
        export BMUXLIB_INPUT_DUMP_DESC=y
        export BMUXLIB_INPUT_DUMP_FRAME_DESC=y
        export NEXUS_USE_7364_SV=y
	export PLATFORM=97364
	export SAGE_SUPPORT=n
        export RAP_IQA_TEST_SUPPORT=y

elif [ $chipnum == "7250" ]
then
        export MEDIA_ASF_SUPPORT=y
        export MEDIA_AVI_SUPPORT=y
        export MEDIA_FLV_SUPPORT=y
	export PLAYBACK_IP_SUPPORT=n
	export VIDEO_ENCODER_SUPPORT=y
	export BDSP_ENCODER_SUPPORT=y
	export BDSP_ENCODER_ACCELERATOR_SUPPORT=y
        export NEXUS_POWER_MANAGEMENT=n
        export BMUXLIB_INPUT_DUMP_DESC=y
        export BMUXLIB_INPUT_DUMP_FRAME_DESC=y
        export NEXUS_USE_7250_SV=y
	export PLATFORM=97250
	export SAGE_SUPPORT=n
	export BDSP_AACSBR_SUPPORT=n
	export BDSP_MS10_SUPPORT=n
	export BDSP_MS11_SUPPORT=n
	export BDSP_MS12_SUPPORT=A
	export BDSP_AC4_SUPPORT=y
	export NEXUS_ARM_AUDIO_SUPPORT=y
