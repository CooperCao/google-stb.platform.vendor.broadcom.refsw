<!--  This file is used for udp support only. It allows you to stream multiple udp streams with ip_streamer.
   Example: nexus ip_streamer -c <configFile.xml> . If you use this option http will be disabled for ip_streamer.
  -->
<ipStreamerConfig version="14.4">
  <globalConfig>
    <interfaceName>eth0</interfaceName>
    <disableFrontends>true</disableFrontends>
    <enableVerbose>false</enableVerbose>
    <videosDirectory>/data/videos</videosDirectory>
  </globalConfig>
  <udp numStreams="8">
    <stream id="1">
     <uri>udp://224.1.1.1:1234/data/videos/toondisney.mpg</uri>
    </stream>
    <stream id="2">
     <uri>udp://224.1.1.2:1234/data/videos/AbcMpeg2HD.mpg</uri>
    </stream>
    <stream id="3">
     <uri>udp://224.1.1.3:1234/data/videos/toondisney.mpg</uri>
    </stream>
    <stream id="4">
     <uri>udp://224.1.1.4:1234/data/videos/AbcMpeg2HD.mpg</uri>
    </stream>
    <stream id="5">
     <uri>udp://224.1.1.5:1234/data/videos/toondisney.mpg</uri>
    </stream>
    <stream id="6">
     <uri>udp://224.1.1.6:1234/data/videos/AbcMpeg2HD.mpg</uri>
    </stream>
    <stream id="7">
     <uri>udp://224.1.1.7:1234/data/videos/toondisney.mpg</uri>
    </stream>
    <stream id="8">
     <uri>udp://224.1.1.8:1234/data/videos/AbcMpeg2HD.mpg</uri>
    </stream>
  </udp>
  <!--  Todo.
  <http>
    <numThreads>3</numThreads>
    <portNum>80</portNum>
  </http>
    -->
</ipStreamerConfig>
