TARGETNAME=$(NIC_WL_EXE)
TARGETTYPE=PROGRAM

SOURCES    =  wfa_ca.c 
ASD_LIB =$(SRCBASE)/tools/Wifi_ASD_test/asd_xp/obj/checked

vpath %.c $(SRCBASE)/tools/Wifi_ASD_test/ca \
          $(SRCBASE)/shared \
	  $(SRCBASE)/tools/epictrl \
          $(SRCBASE)/wl/exe \
	  $(WLAN_StdSrcDirsR) $(WLAN_ComponentSrcDirsR) \
	  $(SRCBASE)/tools/misc

TARGETLIBS =  $(ASD_LIB)/ASD_XP.lib ws2_32.lib wsock32.lib iphlpapi.lib

INCLUDES =  ../inc $(SRCBASE)/include $(SRCBASE)/../components/shared $(WLAN_ComponentIncDirsR)

#ifdef BCMWPA2
C_DEFINES += -DBCMWPA2
#endif

ifeq ($(WLTEST),1)
C_DEFINES += -DSERDOWNLOAD
endif

C_DEFINES +=-DWIN32
C_DEFINES +=-DNDEBUG
C_DEFINES +=-DWIN32_LEAN_AND_MEAN
C_DEFINES +=-D_CONSOLE
#C_DEFINES +=-DRelease
#C_DEFINES +=-DLTCG
#C_DEFINES +=-DGL
C_DEFINES +=-DWFA_WMM_AC 
C_DEFINES +=-DWFA_WMM_EXT 
C_DEFINES +=-DWFA_WMM_PS_EXT 
#C_DEFINES +=-DWFA_WMM_VOICE
C_DEFINES +=-D_MT
