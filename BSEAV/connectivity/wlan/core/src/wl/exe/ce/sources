TARGETNAME=wl

include sources.common

ASD		?= 0
RWL		?= 1	
C_DEFINES +=  -DWIN32 
ifneq ($(RWL),0)
C_DEFINES +=  -DRWL_DONGLE -DRWL_SOCKET -DRWL_WIFI
endif
ifeq ($(ASD),1)
C_DEFINES += -DRWLASD
endif
C_DEFINES +=-D_MD
C_DEFINES +=-D_REENTRANT
C_DEFINES +=-D_CONSOLE

SOURCES=wlu.c wlu_common.c wlu_ndis.c wlu_client_shared.c wlu_cmd.c wlu_iov.c ir_ce.c bcmutils.c bcmwifi_channels.c  bcmstdlib.c wlu_pipe.c wlu_pipe_win32.c miniopt.c bcmxtlv.c bcm_app_utils.c wlu_subcounters.c

ifeq ($(ASD),1)
OTHER_SOURCES:=sources.library
endif
OTHER_SOURCES+=sources.ttcp 
ifneq ($(RWL),0)
OTHER_SOURCES+=sources.dongle sources.socket sources.wifi
endif
