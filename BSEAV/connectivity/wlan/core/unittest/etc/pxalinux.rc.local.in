ifconfig eth0 $IPADDR netmask $NETMASK broadcast $BROADCAST
route del default 2>/dev/null
route add default gw $GATEWAY

killall -9 ushd
ushd -d
if pidof iperf
then
   killall -9 iperf
else
   while :; do iperf -s; done &
fi

