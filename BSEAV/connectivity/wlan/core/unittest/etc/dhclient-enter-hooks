# Deflect attempts to set a default route on a non-backbone interface.
# The backbone interface is defined to be the one whose DHCP server is
# offering a domainname, which is not likely to be a device under
# test.

if [ -z $new_domain_name ]; then
	_new_routers="$new_routers"
	unset new_routers
fi
