#!/bin/sh -e
#
# $Id$
# UTF test cron wrapper for running on a system without direct access
# to SGE.  NOT a general purpose launcher.  This is targeted at
# simplifying the hwnbuild crontab.

log="/var/tmp/hnd/utf_$1.log"; shift
exec >$log 2>&1
exec /tools/bin/ssh utfdev src/tools/unittest/utf_cron.sh "$@" \
    -email hnd-utf-list
