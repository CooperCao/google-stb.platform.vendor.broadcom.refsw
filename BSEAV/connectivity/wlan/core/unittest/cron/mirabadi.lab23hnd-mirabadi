# StaNightly
5	2	* * 2,4,6	src/tools/unittest/cron/launch MCJ Test/StaNightly.test -utfconf MCJ -title 'MCJ 43228 Max Throughput StaNightly' -sta '43228-DUT' -ap '4360-SoftAP' -apdate '2014.3.27.0' -rateonly -perfonly
7	2	* * 2,4,6	src/tools/unittest/cron/launch MCJ Test/StaNightly.test -utfconf MCJ -title 'MCJ 43228 Max Throughput GO StaNightly' -sta '43228-GO' -ap '4360-SoftAP' -apdate '2014.3.27.0' -rateonly -perfonly
9	2	* * 2,4,6	src/tools/unittest/cron/launch MCJ Test/P2PQoSNightly.test -utfconf MCJ -title 'MCJ 43228 Max Throughput TCP WLAN Only' -sta '43228-DUT' -ap '4360-SoftAP' -apdate '2014.3.27.0' -ap_chan 36/80 -fb1 -pm_mode 0 -no_ampdu_dump
11	2	* * 2,4,6	src/tools/unittest/cron/launch MCJ Test/P2PQoSNightly.test -utfconf MCJ -title 'MCJ 43228 Max Throughput UDP WLAN Only' -sta '43228-DUT' -ap '4360-SoftAP' -apdate '2014.3.27.0' -ap_chan 36/80 -fb1 -wlan_bandwidth_BE 1.2g -wlan_packet_size_BE 1470 -run_qos -qos_tests '[WLAN:BE:RX:0:25]\|[WLAN:BE:TX:0:25]\|[WLAN:BE:BI:0:25]' -pm_mode 0 -no_ampdu_dump
13	2	* * 2,4,6	src/tools/unittest/cron/launch MCJ Test/P2PQoSNightly.test -utfconf MCJ -title 'MCJ 43228 Max Throughput GO TCP WLAN Only' -sta '43228-GO' -ap '4360-SoftAP' -apdate '2014.3.27.0' -ap_chan 36/80 -fb1 -pm_mode 0 -no_ampdu_dump
15	2	* * 2,4,6	src/tools/unittest/cron/launch MCJ Test/P2PQoSNightly.test -utfconf MCJ -title 'MCJ 43228 Max Throughput GO UDP WLAN Only' -sta '43228-GO' -ap '4360-SoftAP' -apdate '2014.3.27.0' -ap_chan 36/80 -fb1 -wlan_bandwidth_BE 1.2g -wlan_packet_size_BE 1470 -run_qos -qos_tests '[WLAN:BE:RX:0:25]\|[WLAN:BE:TX:0:25]\|[WLAN:BE:BI:0:25]' -pm_mode 0 -no_ampdu_dump
