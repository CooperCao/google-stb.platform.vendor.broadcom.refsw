#!/bin/env utf
# -*-tcl-*-

#
# UTF test script for testing rexec performance
# $Id: 7d7f87aff997b19ca76dd3b69a58dc26a60fccc7 $
#

package require UTF
package provide UTF::Test::Rexecperf 0.1

UTF::Test Rexecperf {STA {count 5}} {

    # Trigger autoconfiguration
    # Don't use -n since we may need to init, depending on platform
    $STA :
    set cmd [$STA cget -ssh]
    UTF::Message INFO $STA "$cmd: [time {$STA -n :} $count]"
}
