package require UTF
package require UTF::Test::APChanspec
package require UTF::Test::ConnectAPSTA

UTF::Test PR77410 {AP STA} {

    # Configure AP
    APChanspec $AP 36

    $STA reload

    ConnectAPSTA $AP $STA -security wep
    UTF::Multiperf [list $STA [$AP lan]] -t 2

    ConnectAPSTA $AP $STA -security tkippsk

    ConnectAPSTA $AP $STA -security wep
    UTF::Multiperf [list $STA [$AP lan]] -t 2
}

