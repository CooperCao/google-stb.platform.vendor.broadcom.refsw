#!/bin/env utf
# -*-tcl-*-

#
# PR62968 UTF: PM 2 Linux STA -> AP -> STA throughput intermittently
# drops down to PM 1 levels
#
# $Id$
# $Copyright Broadcom Corporation$
#

package require UTF
package require UTF::Test::ConnectAPSTA
package require UTF::Test::Perf-MIMO

UTF::Test PR62968 {} {

    ConnectAPSTA 4705/4322 [list sta1 sta2]
    sta1 wl PM 2
    sta2 wl PM 2
    Perf-MIMO sta1 sta2 -ro "PM 2" -rate autox100 -frameburst 1 \
	-window 524288 -note "WLAN->WLAN"
}

